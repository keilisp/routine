<!DOCTYPE html>
<html lang="en">
<head>
     <meta charset="UTF-8">
     <title></title>
</head>
<body>
      <h1>Posts</h1>  
	  <% posts.forEach(function(post) { %>
		<div>

		<div><h2><%=post.title%></h2></div>
		<div>
		  <%=post.price%>
		</div>
		<div>
		   <%=post.description%>
		</div>
		<div>
		  <%=post.location%>
		</div>
		<div><a href="/posts/<%= post.id %>">View more</a></div>

		<hr>

		</div>
	  <% }); %>
</body>
</html>
